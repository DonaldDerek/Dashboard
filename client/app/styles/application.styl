#main
  margin-top: 100px
  margin-bottom: 30px
#users
  margin-left: 20px
.user-container
  margin-right: 20px
  width: 200px
  height: 420px
  margin-bottom: 20px
  background: rgba(255, 255, 255, 0.7)
  box-shadow: 0 1px 13px rgba(0, 0, 0, 0.3)
  border-radius: 10px
  text-align: center
  a
    color: #9595F1
  h4
    font-family: 'Roboto'
    color: #B4245E
    font-size: 24px;
    font-weight: normal;
    text-shadow: 1px 1px 1px #AAA;
    color: #3B3B3D;
  .blog
    border-top: 1px solid rgba(0, 0, 0, 0.1)
    padding: 5px
  .social
    border-top: 1px solid rgba(0, 0, 0, 0.1)
    padding: 5px
    .entry
      display: inline-block
      margin-right: 7px
      p
        margin-bottom: 0px


